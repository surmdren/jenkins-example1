pipeline {
    agent any
    stages {
        stage('Example') {
            steps {
                script {
                    sh "echo ${env.CHANGE_ID}"
                    sh "echo this is develop branch!"
                    publishChecks name: 'ci-publisher', summary: 'ci-publisher', text: 'ci-publisher', title: 'ci-publisher'
                }
            }
        }
    }
}
